<?xml version="1.0" encoding="UTF-8" ?>
<taglib version="3.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee/web-jsptaglibrary_2_1.xsd">
	<tlib-version>1.0</tlib-version>
	<short-name>cctg</short-name>
	<uri>custom-cinema-tags</uri>
	<tag>
		<name>Paginator</name>
		<tag-class>by.htp.epam.cinema.web.tags.Paginator
		</tag-class>
		<body-content>empty</body-content>
		<attribute>
			<name>count</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>step</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>urlprefix</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<name>CheckSeatExistence</name>
		<tag-class>by.htp.epam.cinema.web.tags.CheckSeatExistence</tag-class>
		<body-content>empty</body-content>
		<attribute>
			<name>seats</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>row</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>column</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<name>DisplayGenresBlock</name>
		<tag-class>by.htp.epam.cinema.web.tags.DisplayGenresBlockTag
		</tag-class>
		<body-content>empty</body-content>
	</tag>
</taglib>